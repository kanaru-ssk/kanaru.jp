---
import BaseLayout from "@/layouts/BaseLayout.astro";
import NotFoundLayout from "@/layouts/NotFoundLayout.astro";
import { getDiffYear } from "@/lib/date";
import { DEFAULT_LANG, getDictionary } from "@/lib/lang";

const yearOfExperience = getDiffYear(new Date("2021-02"), new Date());
const { title, descriptions } = getDictionary(DEFAULT_LANG, yearOfExperience);
---

<BaseLayout
  lang={DEFAULT_LANG}
  title={`404 Not Found | ${title}`}
  descriptions={descriptions}
>
  <NotFoundLayout />
</BaseLayout>
